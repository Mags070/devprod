{
  "name": "devprod",
  "displayName": "devprod",
  "description": "AI: Review the Current File",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
  "menus": {
    "editor/context": [
      {
        "command": "devprod.gendoc",
        "group": "navigation"
      },
      {
        "command": "devprod.addcomments",
        "group": "navigation"
      },
      {
        "command": "devprod.fix",
        "group": "navigation"
      },
      {
        "command": "devprod.explainerror",
        "group": "navigation"
      },
      {
        "command": "devprod.explaincode",
        "group":"navigation"
      }
    ]
  },

    "commands": [
      {
        "command": "devprod.gendoc",
        "title": "AI Documentation"
      },
      {
        "command": "devprod.addcomments",
        "title": "AI comments"
      },
      {
        "command": "devprod.fix",
        "title": "AI optimization"
      },
      {
        "command": "devprod.explainerror",
        "title": "AI debugging"
      },
      {
        "command": "devprod.livecode",
        "title":"AI live code suggestion"
      },
      {
        "command": "devprod.explaincode",
        "title": "AI codespace explainer"
      }
    ]
  },
"activationEvents": [
    "onCommand:devprod.debug",
     "onStartupFinished",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "workspaceContains:**/*"
      ],
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.106.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "mocha": "^11.7.5"
  },
  "dependencies": {
    "@google/genai": "^1.29.1",
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.2.3"
  }
}
